${extend_resource}
| project type,zones, sku=sku.name, sku.capacity, properties.provisioningState, properties.publicNetworkAccess,
properties.status,properties.minimumTlsVersion,properties.zoneRedundant,properties.disableLocalAuth,properties.serviceBusEndpoint,GeoRedundant=properties.privateEndpointConnections
| where type =='microsoft.servicebus/namespaces'
| summarize ResourceTotal=count(Name), NotPremium=dcountif(Name, SkuName!= "Premium"), NoMultipleZones=dcountif(Name, Zones != "null"), NotSucceeded=dcountif(Name, State != "Succeeded")
,NoAVZone=dcountif(Name, AvailabilityZone==false),NoMinimumTLSVersion=dcountif(Name,MinTLSVersion!=1.2), CapacityNotConfigured=dcountif(Name, Capacity=="Not configured")